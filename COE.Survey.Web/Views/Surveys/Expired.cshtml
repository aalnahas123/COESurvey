@using COE.Common.Model

@model Survey

@{
    ViewBag.Title = "Survey";
}

<title>Use options to customize the Survey Editor, Survey Builder Example</title>
<meta name="viewport" content="width=device-width, initial-scale=1">
<script src="https://unpkg.com/jquery"></script>
<script src="~/Content/surveyjs/survey.jquery.min.js"></script>
<link rel="stylesheet" href="https://unpkg.com/bootstrap@3.3.7/dist/css/bootstrap.min.css">

<style>
    .modal-dialog {
        left: auto !important;
    }
</style>

<div class="row">
    <div class="col-md-12">
        <div class="portlet box green">
            <div class="portlet-title">
                <div class="caption">
                    <i class="fa fa-question-circle"></i> @Model.SurveyTitle
                </div>
                <div class="tools">
                    <a href="javascript:;" class="collapse" data-original-title="" title=""> </a>
                </div>
            </div>
            <form class="portlet-body form">
                <!-- BEGIN FORM-->
                <div class="form-body">
                    <div class="row">
                        <div class="col-md-12">
                            <h3 style="text-align: center;">لقد إنتهت صلاحية هذا الإستبيان</h3>
                        </div>
                    </div>
                    <a class="btn btn-primary" href="/surveys"><span>Back</span></a>

                </div>
            </form>
        </div>
    </div>
</div>



